{
  "name": "crebs86/acl-laravel",
  "description": "A simple and complete Control Panel with ACL for Laravel >= 5.5",
  "keywords": [
    "laravel",
    "acl",
    "control panel",
    "auth",
    "roles",
    "permission"
  ],
  "homepage":"https://github.com/crebs86/acl-lavarel",
  "license": "MIT",
  "authors": [
    {
      "name": "Crebs86",
      "email": "crebs.m@gmail.com"
    }
  ],
  "require": {
    "illuminate/console": "~5.0",
    "illuminate/support": "~5.0",
    "illuminate/cache": "~5.0",
    "php": ">=7.0.0"
  },
    "autoload": {
    "classmap": [
    ],
    "psr-4": {
      "Crebs86\\Acl\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Crebs86\\Acl\\AclProvider"
      ],
      "aliases": {
        "Acl": "Crebs86\\Acl\\Facades\\Acl"
      }
    }
  }
}
